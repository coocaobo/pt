<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_ROLE" table="BASE_ROLE">
		<id name="ROLEID" type="java.lang.Long">
	    	<column name="ROLEID"/>
			<generator class="increment" />
		</id>
		<property name="ROLENAME" type="java.lang.String" length="32" />
		<property name="DR" type="java.lang.String" length="1" />
	</class>
</hibernate-mapping>