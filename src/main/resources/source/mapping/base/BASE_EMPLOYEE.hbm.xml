<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_EMPLOYEE" table="BASE_EMPLOYEE">
		<id name="EMPID" type="java.lang.Long">
	    	<column name="EMPID"/>
			<generator class="increment" />
		</id>
		<property name="EMPNAME" type="java.lang.String" length="32" />
		<property name="EMPBIRTHDAY" type="java.util.Date"  />
		<property name="EMPPHONE" type="java.lang.String" length="32" />
		<property name="EMPSEX" type="java.lang.String" length="2" />
		<property name="EMPADDRESS" type="java.lang.String" length="100" />
		<property name="EMPEMAIL" type="java.lang.String" length="50" />
		<property name="DR" type="java.lang.String" length="1" />
		<property name="EMPREMARK" type="java.lang.String" length="200" />
	</class>
</hibernate-mapping>