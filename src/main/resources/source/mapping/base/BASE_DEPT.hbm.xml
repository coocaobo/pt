<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_DEPT" table="BASE_DEPT">
		<id name="DEPTID" type="java.lang.String">
	    	<column name="DEPTID"/>
			<generator class="increment" />
		</id>
		<property name="PID_DEPTID" type="java.lang.String" length="32" />
		<property name="DEPTNAME" type="java.lang.String" length="32" />
		<property name="DEPTADMINUSER" type="java.lang.String" length="32" />
		<property name="DR" type="java.lang.String" length="1" />
	</class>
</hibernate-mapping>