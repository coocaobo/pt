<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_ACTION" table="BASE_ACTION">
		<id name="ACTIONID" type="java.lang.Long">
	    	<column name="ACTIONID"/>
			<generator class="increment" />
		</id>
		<property name="MENUID" type="java.lang.Long" />
		<property name="ACTIONCMD" type="java.lang.String" length="32" />
		<property name="ACTIONNAME" type="java.lang.String" length="32" />
		<property name="PY" type="java.lang.String" length="32" />
		<property name="ACTIONICON" type="java.lang.String" length="32" />
		<property name="DR" type="java.lang.String" length="1" />
		<property name="INTURN" type="java.lang.Integer" />
	</class>
</hibernate-mapping>