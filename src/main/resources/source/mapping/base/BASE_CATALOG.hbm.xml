<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_CATALOG" table="BASE_CATALOG">
		<id name="CATID" type="java.lang.Long">
	    	<column name="CATID"/>
			<generator class="increment" />
		</id>
		<property name="APPID" type="java.lang.Long"/>
		<property name="CATNAME" type="java.lang.String" length="32" />
		<property name="PY" type="java.lang.String" length="32" />
		<property name="CATICON" type="java.lang.String" length="32" />
		<property name="DR" type="java.lang.String" length="1" />
		<property name="INTURN" type="java.lang.Integer" />
	</class>
</hibernate-mapping>