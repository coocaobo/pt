<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_FILE_APP" table="BASE_FILE_APP">
		<id name="FILEAPPID" type="java.lang.Long">
	    	<column name="FILEAPPID"/>
			<generator class="increment" />
		</id>
		<property name="APPTYPE" type="java.lang.String" length="32" />
		<property name="APPPKEY" type="java.lang.String" length="32" />
		<property name="FILEID" type="java.lang.Long" length="1" />
	</class>
</hibernate-mapping>