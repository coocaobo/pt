<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_ROLEPERMISSION" table="BASE_ROLEPERMISSION">
		<id name="RPID" type="java.lang.Long">
	    	<column name="RPID"/>
			<generator class="increment" />
		</id>
		<property name="ROLEID" type="java.lang.Long" length="32" />
		<property name="APPTYPE" type="java.lang.String" length="2" />
		<property name="APPID" type="java.lang.Long" length="32" />
	</class>
</hibernate-mapping>