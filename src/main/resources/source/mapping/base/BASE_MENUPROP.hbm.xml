<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="BASE_MENUPROP" table="BASE_MENUPROP">
		<id name="MPID" type="java.lang.Long">
	    	<column name="MPID"/>
			<generator class="increment" />
		</id>
		<property name="MENUID" type="java.lang.Long" length="32" />
		<property name="MPKEY" type="java.lang.String" length="100" />
		<property name="MPVAL" type="java.lang.String" length="200" />
	</class>
</hibernate-mapping>